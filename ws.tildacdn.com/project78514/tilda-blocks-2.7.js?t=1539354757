function t607_checkAnchorLinks(recid){if(window.innerWidth>=960){var rec=document.getElementById('rec'+recid);var navLinks=rec?rec.querySelectorAll('.t607__list_item a[href*="#"]'):[];navLinks=Array.prototype.filter.call(navLinks,function(navLink){return!navLink.classList.contains('tooltipstered')});if(navLinks.length){t607_catchScroll(navLinks)}}}
function t607_catchScroll(navLinks){var linksAndSections=t607_createSectionAndLinkList(navLinks);window.addEventListener('resize',t_throttle(function(){linksAndSections=t607_getSectionOffset(linksAndSections);linksAndSections.forEach(function(obj){var link=obj.link;link.onclick=function(e){e.preventDefault();t607_scrollToEl(obj.sectionTopPos)}})},100));linksAndSections.forEach(function(obj){var link=obj.link;link.onclick=function(e){e.preventDefault();t607_scrollToEl(obj.sectionTopPos)}});t607_highlightNavLinks(linksAndSections);window.addEventListener('scroll',t_throttle(function(){t607_highlightNavLinks(linksAndSections)},100));document.addEventListener('keydown',function(e){var direction='';if(e.which===38||e.which===33)direction='top';if(e.which===40||e.which===34)direction='bottom';if(!direction)return;linksAndSections.forEach(function(obj){if(obj.link.classList.contains('t-active')){switch(direction){case 'top':if(obj.index!==0){var prevObj=linksAndSections.find(function(el){return el.index===obj.index-1});t607_scrollToEl(prevObj.sectionTopPos)}
break;case 'bottom':if(obj.index!==linksAndSections.length-1){var nextObj=linksAndSections.find(function(el){return el.index===obj.index+1});t607_scrollToEl(nextObj.sectionTopPos)}
break}}})})}
function t607_createSectionAndLinkList(navLinks){var indexValue=0;var linksAndSections=navLinks.map(function(linkEl){var currentSection=t607_getSectionByHref(linkEl);if(!currentSection)return null;return{link:linkEl,section:currentSection,index:indexValue++,}});linksAndSections=linksAndSections.filter(function(obj){return obj});linksAndSections.sort(function(a,b){return a.section.getBoundingClientRect().top-b.section.getBoundingClientRect().top});return t607_getSectionOffset(linksAndSections)}
function t607_getSectionOffset(linksAndSections){var menu=document.querySelector('.t607');var additionalOffsetTop=menu?menu.getAttribute('data-offset-top')||'0':'0';additionalOffsetTop=parseInt(additionalOffsetTop,10);var cashedEndValue=0;return linksAndSections.map(function(obj,i){var lastSectionEndValue=Math.max(document.body.scrollHeight,document.documentElement.scrollHeight,document.body.offsetHeight,document.documentElement.offsetHeight,document.body.clientHeight,document.documentElement.clientHeight);var sectionHalfHeight=obj.section.offsetHeight/2;var sectionTop=obj.section.getBoundingClientRect().top+window.pageYOffset;var sectionStartValue=i===0?sectionTop-sectionHalfHeight-additionalOffsetTop:cashedEndValue;var sectionEndValue=i===linksAndSections.length-1?lastSectionEndValue:sectionTop+sectionHalfHeight-additionalOffsetTop;cashedEndValue=sectionEndValue;return{link:obj.link,section:obj.section,index:obj.index,sectionTopPos:sectionTop-additionalOffsetTop,sectionStart:sectionStartValue,sectionEnd:sectionEndValue,}})}
function t607_getSectionByHref(curlink){if(!curlink)return;var curLinkValue=curlink.href?curlink.href.replace(/\s+/g,'').replace(/.*#/,''):'';if(curlink.href&&curlink.matches('[href*="#rec"]')){return document.getElementById(curLinkValue)}else{var recordElements=document.querySelectorAll('.r[data-record-type="215"]');recordElements=Array.prototype.filter.call(recordElements,function(element){return element.querySelector('a[name="'+curLinkValue+'"]')});return recordElements.length?recordElements[0]:null}}
function t607_highlightNavLinks(linksAndSections){linksAndSections.forEach(function(obj){if(window.pageYOffset>=obj.sectionStart&&window.pageYOffset<obj.sectionEnd){obj.link.classList.add('t-active')}else{obj.link.classList.remove('t-active')}})}
function t607_scrollToEl(elTopPos){if(elTopPos===window.pageYOffset)return!1;var scrollHeight=Math.max(document.body.scrollHeight,document.documentElement.scrollHeight,document.body.offsetHeight,document.documentElement.offsetHeight,document.body.clientHeight,document.documentElement.clientHeight);var maxTopPosViewport=scrollHeight-window.innerHeight;if(elTopPos<0)elTopPos=0;if(elTopPos>maxTopPosViewport)elTopPos=maxTopPosViewport;var stepDivider=20;var duration=400/stepDivider;var difference=window.pageYOffset;var step=Math.abs(window.pageYOffset-elTopPos)/duration;var direction=window.pageYOffset>elTopPos?'top':'bottom';var timerID=setInterval(function(){if(direction==='top'){difference-=step}else{difference+=step}
window.scrollTo(0,difference);document.body.setAttribute('data-scrollable','true');if(direction==='top'&&difference<=elTopPos||direction==='bottom'&&difference>=elTopPos){document.body.removeAttribute('data-scrollable');window.scrollTo(0,elTopPos);clearInterval(timerID)}},stepDivider)}
